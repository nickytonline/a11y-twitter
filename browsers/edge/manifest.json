{
  "name": "a11y Twitter",
  "description": "Accessible reminders for Twitter.",
  "version": "1.2",
  "manifest_version": 3,
  "browser_action": {
    "default_icon": "assets/icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.twitter.com/*"],
      "run_at": "document_idle",
      "js": ["contentScript.js"]
    }
  ]
}
